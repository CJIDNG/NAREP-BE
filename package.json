{
  "name": "narep-be",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=test npm run reset:db:test && NODE_ENV=test nyc --reporter=html --reporter=text mocha --require @babel/register src/tests/*.spec.js --exit",
    "server": "DEBUG=dev nodemon --exec babel-node src/server.js",
    "migrate": "sequelize db:migrate",
    "migrate:undo": "sequelize db:migrate:undo:all",
    "seedall": "sequelize db:seed:all",
    "reset:db": "npm run migrate:undo && npm run migrate && npm run seedall",
    "reset:db:test": "NODE_ENV=test npm run migrate:undo && npm run migrate && npm run seedall ",
    "dev:migrate": "cross-env NODE_ENV=test && sequelize db:migrate",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CJIDNG/NAREP-BE.git"
